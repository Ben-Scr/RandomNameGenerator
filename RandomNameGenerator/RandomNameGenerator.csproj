<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
		
		<PackageId>RandomNameGenerator</PackageId>
		<Version>1.0.0</Version>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>Ben Schneider</Authors>
		<Company>Ben Schneider</Company>
		<Description>A very safe C# Password Generator and Hasher Library</Description>

		<PackageProjectUrl>https://github.com/Ben-Scr/RandomNameGenerator</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Ben-Scr/RandomNameGenerator</RepositoryUrl>
		<RepositoryType>git</RepositoryType>

		<PackageTags>
			Name Generation;
			Random;
		</PackageTags>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageIcon>icon.png</PackageIcon>
  </PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="Resources\FirstNames.txt" />
		<EmbeddedResource Include="Resources\LastNames.txt" />
	</ItemGroup>

</Project>
